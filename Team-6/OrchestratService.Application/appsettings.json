{
	"Serilog": {
		"Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft": "Warning",
				"Microsoft.Hosting.Lifetime": "Warning",
				"MassTransit": "Warning"
			}
		},
		"WriteTo": [
			{ "Name": "Console" },
			{
				"Name": "Seq",
				"Args": {
					"serverUrl": "http://localhost:5341",
					"apiKey": null
				}
			}
		],
		"Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
		"Properties": {
			"Application": "OrchestratService"
		}
	},
	"RabbitMqOptions": {
		"Host": "localhost",
		"Username": "guest",
		"Password": "guest",
		"ClientMessageEventQueue": "client-message-event-queue",
		"ConversationEventQueue": "conversation-event-queue",
		"DefineAgentEvent": "define-agent-event"
	}
}