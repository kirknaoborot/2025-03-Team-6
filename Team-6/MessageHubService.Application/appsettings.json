{
	"Serilog": {
		"Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft": "Warning",
				"Microsoft.Hosting.Lifetime": "Information"
			}
		},
		"WriteTo": [
			{ "Name": "Console" },
			{
				"Name": "Seq",
				"Args": {
					"serverUrl": "http://localhost:5341",
					"apiKey": null
				}
			}
		],
		"Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
		"Properties": {
			"Application": "MessageHubService.Application"
		}
	},
	"RabbitMqOptions": {
		"Host": "localhost",
		"Username": "guest",
		"Password": "guest"
	},
	"ReceiveEndpointOptions": {
		"QueueName": "ClientMessages",
		"PrefetchCount": 4,
		"ExchangeName": "messages"
	},
